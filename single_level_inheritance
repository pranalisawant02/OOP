#include<iostream>
using namespace std;

class A{
	public:
		int a,b;
		void get()
		{
			cout<<"Enter two numbers:"<<endl;
			cin>>a>>b;
		}
	};
//single inheritance
class B:public A
{
	public:
		void sum()
		{
			cout<<"Addition:"<<a+b<<endl;
		}

};
int main()
{
	B obj;
	obj.get();
	obj.sum();
	return 0;
}
