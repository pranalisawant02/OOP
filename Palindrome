#include <iostream>
using namespace std;

int main() {
    // Write C++ code here
   int n=121
   ;
   int t=n;
   int rev=0;
   
   while(t>0)
   {
       int dig=t%10;
       rev=rev*10+dig;
       t/=10;
   }
    if (n==rev)
        cout << "Palindrome.";
    else
        cout << "Not Palindrome.";
    return 0;
}
